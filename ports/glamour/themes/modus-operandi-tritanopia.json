{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "color": "#000000",
    "margin": 2
  },
  "block_quote": {
    "color": "#595959",
    "indent": 1,
    "indent_token": "| "
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "color": "#005f5f",
    "bold": true
  },
  "h1": {
    "prefix": "# ",
    "color": "#000000",
    "background_color": "#ccdfff",
    "bold": true
  },
  "h2": {
    "prefix": "## "
  },
  "h3": {
    "prefix": "### "
  },
  "h4": {
    "prefix": "#### "
  },
  "h5": {
    "prefix": "##### "
  },
  "h6": {
    "prefix": "###### ",
    "color": "#595959",
    "bold": false
  },
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true
  },
  "strong": {
    "bold": true
  },
  "hr": {
    "color": "#9f9f9f",
    "format": "\n--------\n"
  },
  "item": {
    "block_prefix": "- "
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[x] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "#003497",
    "underline": true
  },
  "link_text": {
    "color": "#005e8b",
    "bold": true
  },
  "image": {
    "color": "#721045",
    "underline": true
  },
  "image_text": {
    "color": "#595959",
    "format": "Image: {{.text}} ->"
  },
  "code": {
    "prefix": " ",
    "suffix": " ",
    "color": "#005f5f",
    "background_color": "#f2f2f2"
  },
  "code_block": {
    "color": "#224960",
    "margin": 2,
    "chroma": {
      "text": {
        "color": "#000000"
      },
      "error": {
        "color": "#000000",
        "background_color": "#ff8f88"
      },
      "comment": {
        "color": "#595959"
      },
      "comment_preproc": {
        "color": "#973300"
      },
      "keyword": {
        "color": "#531ab6"
      },
      "keyword_reserved": {
        "color": "#cd22bd"
      },
      "keyword_namespace": {
        "color": "#b21100"
      },
      "keyword_type": {
        "color": "#005f5f"
      },
      "operator": {
        "color": "#721045"
      },
      "punctuation": {
        "color": "#224960"
      },
      "name": {
        "color": "#000000"
      },
      "name_builtin": {
        "color": "#3548cf"
      },
      "name_tag": {
        "color": "#0000b0"
      },
      "name_attribute": {
        "color": "#005e8b"
      },
      "name_class": {
        "color": "#000000",
        "underline": true,
        "bold": true
      },
      "name_constant": {
        "color": "#003497"
      },
      "name_decorator": {
        "color": "#973300"
      },
      "name_exception": {
        "color": "#a60000"
      },
      "name_function": {
        "color": "#721045"
      },
      "name_other": {
        "color": "#000000"
      },
      "literal": {
        "color": "#000000"
      },
      "literal_number": {
        "color": "#624416"
      },
      "literal_date": {
        "color": "#004f5f"
      },
      "literal_string": {
        "color": "#2a5045"
      },
      "literal_string_escape": {
        "color": "#3f578f"
      },
      "generic_deleted": {
        "color": "#a60000"
      },
      "generic_emph": {
        "italic": true
      },
      "generic_inserted": {
        "color": "#008900"
      },
      "generic_strong": {
        "bold": true
      },
      "generic_subheading": {
        "color": "#595959"
      },
      "background": {
        "background_color": "#f2f2f2"
      }
    }
  },
  "table": {},
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n-> "
  },
  "html_block": {},
  "html_span": {}
}
