{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "Protesilaos Stavrou",
  "name": "Modus Themes",
  "themes": [
    {
      "appearance": "{meta:appearance}",
      "name": "{meta:theme_title}",
      "style": {
        "accents": [
          "{color:fg-main}",
          "{color:magenta-intense}",
          "{color:cyan-intense}",
          "{color:red-warmer}",
          "{color:yellow-intense}",
          "{color:magenta-cooler}",
          "{color:green-intense}",
          "{color:blue-warmer}"
        ],
        "background": "{color:bg-main}",
        "border": "{color:fg-dim}",
        "border.variant": "{color:fg-dim}",
        "conflict": "{color:fg-changed-intense}",
        "conflict.background": "{color:bg-changed}",
        "conflict.border": "{color:fg-dim}",
        "created": "{color:fg-added-intense}",
        "created.background": "{color:bg-added}",
        "created.border": "{color:fg-dim}",
        "debugger.accent": "{color:red-faint}",
        "deleted": "{color:fg-removed-intense}",
        "deleted.background": "{color:bg-removed}",
        "deleted.border": "{color:fg-dim}",
        "drop_target.background": "{color:bg-active}8c",
        "editor.active_line.background": "{color:bg-hl-line}",
        "editor.active_line_number": "{color:fg-main}",
        "editor.background": "{color:bg-main}",
        "editor.debugger_active_line.background": "{color:bg-hl-line}",
        "editor.document_highlight.bracket_background": "{color:bg-paren-match}8c",
        "editor.document_highlight.read_background": "{color:bg-dim}",
        "editor.document_highlight.write_background": "{color:bg-hover}",
        "editor.foreground": "{color:fg-main}",
        "editor.gutter.background": "{color:bg-dim}",
        "editor.highlighted_line.background": "{color:bg-hl-line}",
        "editor.invisible": "{color:fg-dim}",
        "editor.line_number": "{color:fg-dim}",
        "editor.subheader.background": "{color:bg-mode-line-active}",
        "element.active": "{color:bg-active}",
        "element.background": "{color:bg-dim}",
        "element.disabled": "{color:bg-dim}",
        "element.hover": "{color:bg-hover}",
        "element.selected": "{color:bg-active}",
        "elevated_surface.background": "{color:bg-dim}",
        "error": "{color:red-faint}",
        "error.background": "{color:bg-red-intense}",
        "error.border": "{color:fg-dim}",
        "ghost_element.active": "{color:bg-active}",
        "ghost_element.background": "{color:bg-dim}",
        "ghost_element.disabled": "{color:bg-dim}",
        "ghost_element.hover": "{color:bg-hover}",
        "ghost_element.selected": "{color:bg-active}",
        "hint": "{color:fg-dim}",
        "hint.background": "{color:bg-dim}",
        "hint.border": "{color:fg-dim}",
        "icon": "{color:fg-main}",
        "icon.accent": "{color:blue-cooler}",
        "icon.disabled": "{color:fg-dim}",
        "icon.muted": "{color:fg-alt}",
        "icon.placeholder": "{color:fg-dim}",
        "info": "{color:cyan-cooler}",
        "info.background": "{color:bg-blue-intense}",
        "info.border": "{color:fg-dim}",
        "modified": "{color:fg-changed-intense}",
        "modified.background": "{color:bg-changed}",
        "modified.border": "{color:fg-dim}",
        "panel.background": "{color:bg-main}",
        "players": [
          {
            "background": "{color:bg-region}",
            "cursor": "{color:fg-main}",
            "selection": "{color:fg-main}3d"
          },
          {
            "background": "{color:magenta-intense}",
            "cursor": "{color:magenta-intense}",
            "selection": "{color:magenta-intense}3d"
          },
          {
            "background": "{color:cyan-intense}",
            "cursor": "{color:cyan-intense}",
            "selection": "{color:cyan-intense}3d"
          },
          {
            "background": "{color:red-warmer}",
            "cursor": "{color:red-warmer}",
            "selection": "{color:red-warmer}3d"
          },
          {
            "background": "{color:yellow-intense}",
            "cursor": "{color:yellow-intense}",
            "selection": "{color:yellow-intense}3d"
          },
          {
            "background": "{color:magenta-cooler}",
            "cursor": "{color:magenta-cooler}",
            "selection": "{color:magenta-cooler}3d"
          },
          {
            "background": "{color:green-intense}",
            "cursor": "{color:green-intense}",
            "selection": "{color:green-intense}3d"
          }
        ],
        "predictive": "{color:fg-dim}",
        "predictive.background": null,
        "predictive.border": "{color:fg-dim}",
        "renamed": "{color:fg-changed-intense}",
        "renamed.background": "{color:bg-changed}",
        "renamed.border": "{color:fg-dim}",
        "scrollbar.thumb.background": "{color:bg-mode-line-active}8c",
        "scrollbar.thumb.border": "{color:fg-dim}",
        "scrollbar.thumb.hover_background": "{color:bg-active}",
        "scrollbar.track.background": "{color:bg-dim}",
        "scrollbar.track.border": "{color:fg-dim}",
        "search.match_background": "{color:bg-cyan-intense}",
        "status_bar.background": "{color:bg-mode-line-active}",
        "success": "{color:fg-main}",
        "success.background": "{color:bg-cyan-intense}",
        "success.border": "{color:fg-dim}",
        "surface.background": "{color:bg-dim}",
        "syntax": {
          "attribute": {
            "color": "{color:red-cooler}"
          },
          "boolean": {
            "color": "{color:blue-cooler}"
          },
          "comment": {
            "color": "{color:fg-dim}",
            "font_style": "italic"
          },
          "constant": {
            "color": "{color:blue-cooler}"
          },
          "constructor": {
            "color": "{color:magenta}"
          },
          "embedded": {
            "color": "{color:red-cooler}"
          },
          "function": {
            "color": "{color:magenta}"
          },
          "keyword": {
            "color": "{color:magenta-cooler}"
          },
          "number": {
            "color": "{color:fg-main}"
          },
          "operator": {
            "color": "{color:magenta}"
          },
          "property": {
            "color": "{color:cyan}"
          },
          "string": {
            "color": "{color:blue-warmer}"
          },
          "string.escape": {
            "color": "{color:blue-warmer}"
          },
          "string.regex": {
            "color": "{color:blue-warmer}"
          },
          "string.special": {
            "color": "{color:blue-warmer}"
          },
          "string.special.symbol": {
            "color": "{color:blue-warmer}"
          },
          "tag": {
            "color": "{color:blue-cooler}"
          },
          "text.literal": {
            "color": "{color:cyan-cooler}"
          },
          "title": {
            "color": "{color:yellow-faint}"
          },
          "type": {
            "color": "{color:cyan-cooler}"
          },
          "variable": {
            "color": "{color:cyan}"
          },
          "variable.special": {
            "color": "{color:magenta-cooler}"
          }
        },
        "tab.active_background": "{color:bg-main}",
        "tab.inactive_background": "{color:bg-tab-other}",
        "tab_bar.background": "{color:bg-tab-bar}",
        "terminal.ansi.black": "{color:fg-term-black}",
        "terminal.ansi.blue": "{color:fg-term-blue}",
        "terminal.ansi.bright_black": "{color:fg-term-black-bright}",
        "terminal.ansi.bright_blue": "{color:fg-term-blue-bright}",
        "terminal.ansi.bright_cyan": "{color:fg-term-cyan-bright}",
        "terminal.ansi.bright_green": "{color:fg-term-green-bright}",
        "terminal.ansi.bright_magenta": "{color:fg-term-magenta-bright}",
        "terminal.ansi.bright_red": "{color:fg-term-red-bright}",
        "terminal.ansi.bright_white": "{color:fg-term-white-bright}",
        "terminal.ansi.bright_yellow": "{color:fg-term-yellow-bright}",
        "terminal.ansi.cyan": "{color:fg-term-cyan}",
        "terminal.ansi.dim_black": null,
        "terminal.ansi.dim_blue": null,
        "terminal.ansi.dim_cyan": null,
        "terminal.ansi.dim_green": null,
        "terminal.ansi.dim_magenta": null,
        "terminal.ansi.dim_red": null,
        "terminal.ansi.dim_white": null,
        "terminal.ansi.dim_yellow": null,
        "terminal.ansi.green": "{color:fg-term-green}",
        "terminal.ansi.magenta": "{color:fg-term-magenta}",
        "terminal.ansi.red": "{color:fg-term-red}",
        "terminal.ansi.white": "{color:fg-term-white}",
        "terminal.ansi.yellow": "{color:fg-term-yellow}",
        "terminal.background": "{color:bg-main}",
        "terminal.bright_foreground": "{color:fg-alt}",
        "terminal.dim_foreground": "{color:fg-dim}",
        "terminal.foreground": "{color:fg-main}",
        "text": "{color:fg-main}",
        "text.accent": "{color:blue-cooler}",
        "text.disabled": "{color:fg-dim}",
        "text.muted": "{color:fg-alt}",
        "text.placeholder": "{color:fg-dim}",
        "title_bar.background": "{color:bg-main}",
        "title_bar.inactive_background": "{color:bg-dim}",
        "toolbar.background": "{color:bg-main}",
        "unreachable": "{color:fg-dim}",
        "unreachable.background": "{color:bg-dim}",
        "unreachable.border": "{color:fg-dim}",
        "version_control.added": "{color:bg-added-fringe}",
        "version_control.conflict": "{color:bg-changed-fringe}",
        "version_control.conflict_marker.ours": "{color:bg-removed}",
        "version_control.conflict_marker.theirs": "{color:bg-added}",
        "version_control.deleted": "{color:bg-removed-fringe}",
        "version_control.ignored": "{color:fg-dim}",
        "version_control.modified": "{color:bg-changed-fringe}",
        "version_control.renamed": "{color:bg-changed-fringe}",
        "vim.helix_normal.background": "{color:blue-faint}",
        "vim.helix_normal.foreground": "{color:bg-dim}",
        "vim.insert.background": "{color:green-faint}",
        "vim.insert.foreground": "{color:bg-dim}",
        "vim.normal.background": "{color:blue-faint}",
        "vim.normal.foreground": "{color:bg-dim}",
        "vim.replace.background": "{color:red-faint}",
        "vim.replace.foreground": "{color:bg-dim}",
        "vim.visual.background": "{color:magenta-faint}",
        "vim.visual.foreground": "{color:bg-dim}",
        "warning": "{color:fg-main}",
        "warning.background": "{color:bg-yellow-intense}",
        "warning.border": "{color:fg-dim}"
      }
    }
  ]
}
